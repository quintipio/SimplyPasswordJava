    <property name="temp.jar.path.SimplyPasswordJava.jar" value="${artifacts.temp.dir}/SimplyPasswordJava.jar"/>
  </target>
  
  <target name="artifact.simplypasswordjava:jar" depends="init.artifacts, compile.module.simplypasswordjava" description="Build &#39;SimplyPasswordJava:jar&#39; artifact">
    <property name="artifact.temp.output.simplypasswordjava:jar" value="${artifacts.temp.dir}/SimplyPasswordJava_jar"/>
    <mkdir dir="${artifact.temp.output.simplypasswordjava:jar}"/>
    <jar destfile="${temp.jar.path.SimplyPasswordJava.jar}" duplicate="preserve" filesetmanifest="mergewithoutmain">
      <zipfileset dir="${simplypasswordjava.output.dir}"/>
    </jar>
    <copy file="${temp.jar.path.SimplyPasswordJava.jar}" tofile="${artifact.temp.output.simplypasswordjava:jar}/SimplyPasswordJava.jar"/>
  </target>
  
  <target name="build.all.artifacts" depends="artifact.simplypasswordjava:jar" description="Build all artifacts">
    <mkdir dir="${artifact.output.simplypasswordjava:jar}"/>
    <copy todir="${artifact.output.simplypasswordjava:jar}">
      <fileset dir="${artifact.temp.output.simplypasswordjava:jar}"/>
    </copy>
    
    <!-- Delete temporary files -->
    <delete dir="${artifacts.temp.dir}"/>
  </target>
  
